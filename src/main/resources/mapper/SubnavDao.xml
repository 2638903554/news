<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leehom.news.dao.SubnavDao">
    <select id="selectDetailListByTypeId" resultType="Subnav">
        select * from subnav WHERE nav_id = #{typeId}
    </select>

    <select id="selectByDetailId" resultType="Subnav">
        select * from subnav where subnav_id = #{detailId}
    </select>
</mapper>