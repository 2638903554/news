<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leehom.news.dao.DetailDao">
    <select id="selectDetailListByTypeId" resultType="Detail">
        select * from detail WHERE type_id = #{typeId}
    </select>

    <select id="selectByDetailId" resultType="Detail">
        select * from detail where detail_id = #{detailId}
    </select>
</mapper>